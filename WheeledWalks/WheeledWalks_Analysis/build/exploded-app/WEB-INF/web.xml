<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">

    <servlet>
        <servlet-name>WarmupServlet</servlet-name>
        <servlet-class>com.example.David.WheeledWalks.backend.WarmupServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WarmupServlet</servlet-name>
        <url-pattern>/_ah/start</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>WalkProcessingServlet</servlet-name>
        <servlet-class>com.example.David.WheeledWalks.backend.WalkProcessingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WalkProcessingServlet</servlet-name>
        <url-pattern>/WalkProcessingServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DisplayWalkServlet</servlet-name>
        <servlet-class>com.example.David.WheeledWalks.backend.DisplayWalkServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DisplayWalkServlet</servlet-name>
        <url-pattern>/DisplayWalk</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>